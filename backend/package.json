{
  "name": "backend",
  "version": "1.0.0",
  "description": "graphql backend endpoint api for elemohstores",
  "main": "index.js",
  "scripts": {
    "predev": "yarn types",
    "build": "tsc src --outDir dist",
    "start": "node dist/index.js",
    "pretypes": "ts-node src/utils/buildSchema.js",
    "types": "graphql-to-typescript ./src/data/schema.graphql ./src/types/graphql.d.ts",
    "dev": "nodemon --exec ts-node src/index.ts -e js,ts",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && yarn run build",
    "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "functional coders",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@typegoose/typegoose": "^7.6.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.18.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-compose": "^7.22.1",
    "graphql-compose-connection": "^8.0.1",
    "graphql-compose-mongoose": "^9.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-tools": "^6.2.4",
    "helmet": "^4.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "twilio": "^3.61.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/twilio": "^3.19.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.0.1",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^2.0.6",
    "parcel": "^1.12.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config": "^0.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  }
}
